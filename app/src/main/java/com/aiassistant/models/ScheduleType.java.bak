package com.aiassistant.models;

/**
 * Schedule Type
 * Types of schedules for task execution
 */
public enum ScheduleType {
    /**
     * One-time execution
     */
    ONE_TIME,
    
    /**
     * Recurring execution at fixed intervals
     */
    RECURRING,
    
    /**
     * Daily execution at a specific time
     */
    DAILY,
    
    /**
     * Weekly execution on specific days at a specific time
     */
    WEEKLY,
    
    /**
     * Application event-based execution (app launch, app close)
     */
    APP_EVENT,
    
    /**
     * Execute when device is idle
     */
    IDLE
}